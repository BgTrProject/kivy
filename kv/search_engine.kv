<Search_Engine>:
    FloatLayout:
        orientation: 'vertical'
        padding: [10,50,10,50]
        size_hint: None, None
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        elevation: 10
        spacing: 15
        orientation: 'vertical'

        Button:
            text: "Google Search"
            font_size: 20
            background_color:0.208, 1, 0.973
            pos_hint:{"center_x": 0.5, "top": 3}
            size_hint:(3, .8)
            on_press: root.google_search()

        Button:
            text: "Google Topik"
            font_size: 20
            background_color:0.208, 1, 0.973
            pos_hint:{"center_x": 0.5, "top":2}
            size_hint:(3, .8)
            on_press: root.google_topik()

        Button:
            text:'Bing'
            font_size: 20
            background_color:0.208, 1, 0.973
            pos_hint:{"center_x": 0.5, "top": 1}
            size_hint:(3, .8)
            on_press: root.bing()

        Button:
            text:'Get Newspaper on Google'
            font_size: 20
            background_color:0.208, 1, 0.973
            pos_hint:{"center_x": 0.5, "top": 0}
            size_hint:(3, .8)
            on_press: root.news_google()



        Button:
            text: ""
            background_normal:'image/geri3.png'
            font_size: 18
            pos_hint:{"center_x": -2.4, "top": -1.6}
            size_hint:(0.6,0.6)
            on_press: root.disconnect()
